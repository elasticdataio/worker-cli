{
  "name": "@elasticdataio/worker-cli",
  "description": "CLI of elasticdata.io worker",
  "version": "0.0.1",
  "author": "sergey tkachenko @sergeytkachenko",
  "bin": {
    "ed": "./bin/run"
  },
  "bugs": "https://github.com/elasticdataio/worker-cli/issues",
  "dependencies": {},
  "devDependencies": {},
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/elasticdataio/worker-cli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "ed",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "elasticdataio/worker-cli",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
