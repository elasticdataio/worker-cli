{
  "name": "@elasticdataio/worker-cli",
  "description": "CLI of elasticdata.io worker",
  "version": "0.0.60",
  "author": "sergey tkachenko @elasticdataio",
  "bin": {
    "ed-cli": "./bin/run",
    "ed-server": "./node_modules/@elasticdataio/worker/dist/main.js"
  },
  "bugs": "https://github.com/elasticdataio/worker-cli/issues",
  "dependencies": {
    "@elasticdataio/worker": "^0.0.14",
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.2",
    "axios": "^0.21.1",
    "chalk": "^4.1.0",
    "tslib": "^1.14.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "@oclif/test": "^1.2.8",
    "@types/chai": "^4.2.16",
    "@types/chalk": "^2.2.0",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.17.56",
    "chai": "^4.3.4",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "globby": "^10.0.2",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.9"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/elasticdataio/worker-cli",
  "keywords": [
    "elasticdataio",
    "parser",
    "scraper"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "ed-cli",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "elasticdataio/worker-cli",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts",
  "private": false
}
